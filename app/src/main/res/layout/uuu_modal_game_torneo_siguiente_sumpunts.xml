<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/float_transparent"
    >
    <LinearLayout
        android:layout_gravity="center"
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:background="@drawable/fondo_game_torneo_estud"
        android:orientation="vertical"
        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:text="Resultados del juego"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_weight="2"
            android:fontFamily="@font/digitalt"
            android:textSize="20dp"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="6"
            android:layout_marginHorizontal="10dp"
            android:orientation="vertical"
            >


            <View
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@color/white"
                />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:gravity="center"
                >
                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    />
                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="cantidad"
                    android:textColor="@color/white"
                    android:fontFamily="@font/digitalt"
                    android:gravity="center"
                    />
                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="puntos"
                    android:textColor="@color/white"
                    android:fontFamily="@font/digitalt"
                    android:gravity="center"

                    />




            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@color/white"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:gravity="center"
                >
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:text="Preguntas Correctas"
                    android:fontFamily="@font/digitalt"
                    android:textSize="17dp"
                    android:textColor="@color/white"
                    />
                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />



                <TextView
                    android:id="@+id/cant_correctas"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="7"
                    android:fontFamily="@font/digitalt"
                    android:textColor="@color/white"
                    android:gravity="center"
                    />
                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />
                <TextView
                    android:id="@+id/pts_correctas"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="7"
                    android:fontFamily="@font/digitalt"
                    android:textColor="@color/white"
                    android:gravity="center"

                    />




            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@color/white"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:gravity="center"

                >


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:text="Preguntas falladas"
                    android:fontFamily="@font/digitalt"
                    android:textSize="17dp"
                    android:textColor="@color/white"


                    />

                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />



                <TextView
                    android:id="@+id/cant_falladas"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="7"
                    android:fontFamily="@font/digitalt"
                    android:textColor="@color/white"
                    android:gravity="center"
                    />
                <View
                    android:layout_width="1.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />
                <TextView
                    android:id="@+id/pts_falladas"

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="7"
                    android:fontFamily="@font/digitalt"
                    android:textColor="@color/white"
                    android:gravity="center"

                    />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@color/white"
                />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:gravity="center"

                >



                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:text="Total acumulado"
                    android:fontFamily="@font/digitalt"
                    android:textSize="17dp"
                    android:textColor="@color/white"

                    />







                <View
                    android:layout_width="3dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    />
                <TextView
                    android:id="@+id/total_pts"
                    android:background="@color/white"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:text="7"
                    android:fontFamily="@font/digitalt"
                    android:textColor="@color/adminU"
                    android:gravity="center"
                    android:textSize="20dp"

                    />



            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@color/white"
                />



        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            >
            <Button
                android:id="@+id/btn_salir"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="salir"
                android:fontFamily="@font/digitalt"
                android:background="@drawable/buttonwarning"
                />
            <View
                android:layout_width="60dp"
                android:layout_height="10dp"/>
            <Button
                android:id="@+id/btn_siguiente"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="continuar"
                android:fontFamily="@font/digitalt"
                android:background="@drawable/buttoncrear"
                />
        </LinearLayout>


    </LinearLayout>

</FrameLayout>